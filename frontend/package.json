{
 "name": "pizzaql-frontend",
 "version": "0.0.2",
 "description": "Frontend for PizzaQL",
 "scripts": {
  "dev": "next",
  "build": "next build",
  "start": "next start",
  "test": "xo && stylelint './pages/**/*.js' && ava"
 },
 "main": "pages/index.js",
 "author": "Antoni Kepinski <a@kepinski.me> (https://kepinski.me)",
 "license": "MIT",
 "devDependencies": {
  "@babel/plugin-proposal-class-properties": "^7.3.0",
  "ava": "*",
  "babel-eslint": "^10.0.1",
  "babel-plugin-styled-components": "^1.10.0",
  "enzyme": "^3.8.0",
  "enzyme-adapter-react-16": "^1.9.1",
  "eslint-config-xo-react": "^0.18.0",
  "eslint-plugin-react": "^7.12.4",
  "jsdom": "13.2.0",
  "jsdom-global": "3.0.2",
  "stylelint": "^9.10.1",
  "stylelint-config-recommended": "^2.1.0",
  "stylelint-config-styled-components": "^0.1.1",
  "stylelint-processor-styled-components": "^1.5.2",
  "xo": "*"
 },
 "dependencies": {
  "@babel/register": "^7.0.0",
  "@blueprintjs/core": "^3.13.0",
  "@zeit/next-css": "^1.0.1",
  "apollo-boost": "^0.1.27",
  "auth0-js": "^9.10.0",
  "dayjs": "^1.8.5",
  "formik": "^1.5.0",
  "graphql": "^14.1.1",
  "graphql-request": "^1.8.2",
  "graphql-tag": "^2.10.1",
  "isomorphic-unfetch": "^3.0.0",
  "js-cookie": "^2.2.0",
  "jsonwebtoken": "^8.4.0",
  "modern-normalize": "^0.5.0",
  "next": "^7.0.3",
  "prop-types": "^15.6.2",
  "react": "^16.8.1",
  "react-apollo": "^2.4.1",
  "react-dom": "^16.8.1",
  "react-use-localstorage": "^1.1.0",
  "styled-components": "^4.1.3",
  "styled-theming": "^2.2.0",
  "yup": "^0.26.10"
 },
 "xo": {
  "nodeVersion": ">=10",
  "parser": "babel-eslint",
  "envs": [
   "node",
   "browser"
  ],
  "extends": "xo-react",
  "settings": {
   "react": {
    "version": "16.8"
   }
  },
  "rules": {
   "import/no-unassigned-import": "off",
   "import/no-named-as-default": "off",
   "unicorn/filename-case": "off",
   "react/jsx-sort-props": "off",
   "react/prop-types": "off"
  }
 },
 "ava": {
  "require": [
   "@babel/register",
   "./test/_index"
  ]
 }
}
