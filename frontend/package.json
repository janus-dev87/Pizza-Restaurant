{
 "name": "pizzaql-frontend",
 "version": "0.0.2",
 "description": "Frontend for PizzaQL",
 "scripts": {
  "postinstall": "babel ./node_modules/ky/index.js --out-file ./node_modules/ky/index.js --config-file=./babel.config.js",
  "dev": "next",
  "build": "next build",
  "start": "next start",
  "test": "xo && stylelint './pages/**/*.js' && ava"
 },
 "main": "pages/index.js",
 "author": "Antoni Kepinski <a@kepinski.me> (https://kepinski.me)",
 "license": "MIT",
 "devDependencies": {
  "@babel/cli": "^7.2.3",
  "@babel/core": "^7.2.2",
  "@babel/plugin-proposal-class-properties": "^7.2.3",
  "ava": "^1.1.0",
  "babel-eslint": "^10.0.1",
  "babel-plugin-styled-components": "^1.10.0",
  "enzyme": "^3.8.0",
  "enzyme-adapter-react-16": "^1.7.1",
  "eslint-config-xo-react": "^0.18.0",
  "eslint-plugin-react": "^7.12.3",
  "jsdom": "13.1.0",
  "jsdom-global": "3.0.2",
  "stylelint": "^9.9.0",
  "stylelint-config-recommended": "^2.1.0",
  "stylelint-config-styled-components": "^0.1.1",
  "stylelint-processor-styled-components": "^1.5.2",
  "xo": "^0.24.0"
 },
 "dependencies": {
  "@babel/register": "^7.0.0",
  "@blueprintjs/core": "^3.11.0",
  "auth0-js": "^9.9.0",
  "boolean": "^0.2.0",
  "dayjs": "^1.8.0",
  "fontfaceobserver": "^2.1.0",
  "formik": "^1.4.2",
  "isomorphic-unfetch": "^3.0.0",
  "js-cookie": "^2.2.0",
  "jsonwebtoken": "^8.4.0",
  "ky": "^0.6.0",
  "next": "^7.0.2",
  "prop-types": "^15.6.2",
  "react": "^16.7.0",
  "react-clipboard.js": "^2.0.2",
  "react-dom": "^16.7.0",
  "styled-components": "^4.1.3",
  "yup": "^0.26.7"
 },
 "xo": {
  "nodeVersion": ">=10",
  "parser": "babel-eslint",
  "envs": [
   "node",
   "browser"
  ],
  "extends": "xo-react",
  "settings": {
   "react": {
    "version": "16.7"
   }
  },
  "rules": {
   "import/no-unassigned-import": "off",
   "unicorn/filename-case": "off",
   "react/jsx-sort-props": "off",
   "react/prop-types": "off"
  }
 },
 "ava": {
  "require": [
   "@babel/register",
   "./test/_index"
  ]
 }
}
